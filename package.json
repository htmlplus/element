{
  "name": "@htmlplus/element",
  "version": "0.1.3",
  "license": "MIT",
  "author": "Masood Abdolian <m.abdolian@gmail.com>",
  "description": "Compiler of HTMLPlus",
  "type": "module",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "cpx 'src/**/*.hbs' 'dist'",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "prettier": "prettier --write .",
    "start": "node src/dev/start.js"
  },
  "exports": {
    ".": {
      "default": "./dist/client/index.js"
    },
    "./compiler": {
      "default": "./dist/compiler/index.js"
    },
    "./decorators": {
      "default": "./dist/client/decorators/index.js"
    },
    "./helpers": {
      "default": "./dist/client/helpers/index.js"
    },
    "./runtime": {
      "default": "./dist/runtime/index.js"
    },
    "./services": {
      "default": "./dist/client/services/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/client/index.d.ts"
      ],
      "compiler": [
        "./dist/compiler/index.d.ts"
      ],
      "decorators": [
        "./dist/client/decorators/index.d.ts"
      ],
      "helpers": [
        "./dist/client/helpers/index.d.ts"
      ],
      "runtime": [
        "./dist/runtime/index.d.ts"
      ],
      "services": [
        "./dist/client/services/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">= 12.7.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/htmlplus/element.git"
  },
  "bugs": {
    "url": "https://github.com/htmlplus/element/issues"
  },
  "homepage": "https://github.com/htmlplus/element#readme",
  "dependencies": {
    "@babel/generator": "^7.16.0",
    "@babel/parser": "^7.16.4",
    "@babel/traverse": "^7.16.3",
    "@babel/types": "^7.16.0",
    "@types/node": "^16.11.11",
    "change-case": "^4.1.2",
    "glob": "^7.2.0",
    "handlebars": "^4.7.7",
    "log-update": "^5.0.0",
    "sass": "^1.43.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "uhtml": "^2.7.6"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "cpx": "^1.5.0",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "vite": "^2.7.10"
  }
}
